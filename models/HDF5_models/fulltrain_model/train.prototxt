name: "ResNet-50"

layer {
 	top: "data"
	top: "x1"
	top: "y1"
	top: "x2"
	top: "y2"
	top: "category"
	name: "input"
	type: "HDF5Data" 
	include {
		phase:TRAIN
	}
	hdf5_data_param {
		#source: "../../../data_subsets/HDF5/mini_data/minidata_hdf5_path.txt"
		source: "minidata_hdf5_path.txt"
		batch_size:4
		shuffle:1 
	}
}

layer {
	name: "concat"
	bottom: "x1"
	bottom: "y1"
	bottom: "x2"
	bottom: "y2"
	top:"bbox_position"
	type: "Concat"
	concat_param {
		axis:1
	}
}

#layer {
#	bottom: "label"	
#	top: "bbox_position"
#	top: "category"
